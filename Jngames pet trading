local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Pet trading Free by jngames V1", "Sentinel")
 
--Main
local Main = Window:NewTab("Main")
local MainSection = Main:NewSection("Dupe Pet")

MainSection:NewButton("dupe", "Pet", function()
    -- Script generated by SimpleSpy - credits to exx#9394
    
    local args = {
        [1] = {
            ["10001"] = 0
        }
    }
    
    game:GetService("ReplicatedStorage").Functions.Fuse.FuseRF:InvokeServer(unpack(args))
    end)

    MainSection:NewButton("upgrade req 3", "ButtonInfo", function()
        -- Script generated by SimpleSpy - credits to exx#9394
        
        local args = {
            [1] = {
                ["10011"] = 3
            }
        }
        
        game:GetService("ReplicatedStorage").Functions.Fuse.FuseRF:InvokeServer(unpack(args))
        end)

