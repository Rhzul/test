            [1] = {
                ["10084"] = 1
            }
        }
        
        game:GetService("ReplicatedStorage").Functions.Fuse.FuseRF:InvokeServer(unpack(args))
        end)

        UpgradeSection:NewButton("Upgrade V3", "ButtonInfo", function()
            -- Script generated by SimpleSpy - credits to exx#9394
            
            local args = {
                [1] = {
                    ["10173"] = 1
                }
            }
            
            game:GetService("ReplicatedStorage").Functions.Fuse.FuseRF:InvokeServer(unpack(args))
            end)

--Rainbow
local Rainbow = Window:NewTab("golden and rainbow!")
local RainbowSection = Rainbow:NewSection("Make rainbow pizza and golden pizza requires 6!")

RainbowSection:NewButton("make rainbow pizza", "ButtonInfo", function()
    local args = {
        [1] = "20001",
        [2] = 6,
        [3] = "Rainbow"
    }
    
    game:GetService("ReplicatedStorage"):WaitForChild("Functions"):WaitForChild("Upgrade"):WaitForChild("UpgradeRF"):InvokeServer(unpack(args))
end)
    
    RainbowSection:NewButton("make golden pizza", "ButtonInfo", function()
        local args = {
            [1] = "10001",
            [2] = 6,
            [3] = "Golden"
        }
        
        game:GetService("ReplicatedStorage"):WaitForChild("Functions"):WaitForChild("Upgrade"):WaitForChild("UpgradeRF"):InvokeServer(unpack(args))
    end)

--fuse
local fuse = Window:NewTab("upgrade rainbow pizza")
local fuseSection = fuse:NewSection("upgrade rainbow pizza and golden pizza!")

fuseSection:NewButton("3 rb pizza", "ButtonInfo", function()
    -- Script generated by SimpleSpy - credits to exx#9394
    
    local args = {
        [1] = {
            ["30001"] = 3
        }
    }
    
    game:GetService("ReplicatedStorage").Functions.Fuse.FuseRF:InvokeServer(unpack(args))
    end)

    fuseSection:NewButton("1 golden pizza", "ButtonInfo", function()
        -- Script generated by SimpleSpy - credits to exx#9394
        
        local args = {
            [1] = {
                ["20001"] = 1
            }
        }
        
        game:GetService("ReplicatedStorage").Functions.Fuse.FuseRF:InvokeServer(unpack(args))
        end)

--Colored
local Colored = Window:NewTab("Colored items")
local ColoredSection = Colored:NewSection("Buy colored items!")

ColoredSection:NewButton("Buy colored seahorse 5M", "Requires 5m", function()
    local args = {
        [1] = "purchase",
        [2] = 40058,
        [3] = 1
    }
    
    game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("Coin"):WaitForChild("CoinMarketRE"):FireServer(unpack(args))
end)

