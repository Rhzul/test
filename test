local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("pop it trading Free by jngames V1", "Ocean")
 
--Main
local Main = Window:NewTab("Main")
local MainSection = Main:NewSection("Dupe Pizza")

MainSection:NewButton("dupe Pizza", "ButtonInfo", function()
    -- Script generated by SimpleSpy - credits to exx#9394
    
    local args = {
        [1] = {
            ["10020"] = 0
        }
    }
    
    game:GetService("ReplicatedStorage").Functions.Fuse.FuseRF:InvokeServer(unpack(args))
    end)

    MainSection:NewButton("upgrade pizza", "ButtonInfo", function()
        -- Script generated by SimpleSpy - credits to exx#9394
        
        local args = {
            [1] = {
                ["10001"] = 1
            }
        }
        
        game:GetService("ReplicatedStorage").Functions.Fuse.FuseRF:InvokeServer(unpack(args))
        end)  
        
 --Upgrade
 local Upgrade = Window:NewTab("More")
 local UpgradeSection = Upgrade:NewSection("First have the pop it then upgrade!")

 UpgradeSection:NewButton("Upgrade V1", "ButtonInfo", function()
    -- Script generated by SimpleSpy - credits to exx#9394
    
